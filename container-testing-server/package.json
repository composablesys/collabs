{
  "name": "@collabs/container-testing-server",
  "version": "0.9.1",
  "description": "Simple server for testing Collabs containers",
  "author": "Collabs contributors",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/composablesys/collabs/issues"
  },
  "homepage": "https://github.com/composablesys/collabs/tree/master/container-testing-server#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/composablesys/collabs.git"
  },
  "keywords": [
    "Collabs",
    "collaboration",
    "local-first"
  ],
  "bin": {
    "container-testing-server": "bin/container-testing-server"
  },
  "files": [
    "/build",
    "/keys",
    "/bin",
    "/src"
  ],
  "directories": {
    "lib": "src"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@collabs/collabs": "0.9.1",
    "@collabs/container": "0.9.1",
    "@collabs/ws-client": "0.9.1",
    "@collabs/ws-server": "0.9.1",
    "commander": "^8.2.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/node": "^15.6.1",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.16.2",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "html-webpack-plugin": "^5.3.2",
    "npm-run-all": "^4.1.5",
    "pkg-ok": "^2.3.1",
    "prettier": "^2.2.1",
    "rimraf": "^2.7.1",
    "source-map-loader": "^3.0.0",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.8.0"
  },
  "scripts": {
    "prepack": "npm run clean && npm run build && npm run test",
    "start": "bin/container-testing-server",
    "dev": "npm-run-all dev:*",
    "dev:site": "cross-env TS_NODE_PROJECT='tsconfig-for-webpack-config.json' webpack",
    "dev:server": "npm run build:server",
    "build": "npm-run-all build:*",
    "build:site": "cross-env TS_NODE_PROJECT='tsconfig-for-webpack-config.json' webpack --mode production --devtool source-map",
    "build:server": "tsc --build tsconfig-server.json",
    "test": "npm-run-all test:*",
    "test:pkg": "pkg-ok",
    "test:format": "prettier --check .",
    "fix": "npm-run-all fix:*",
    "fix:format": "prettier --write .",
    "clean": "rimraf build"
  }
}
