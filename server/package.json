{
  "name": "compoventuals-server",
  "version": "0.1.0",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "unpkg": "build/src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/composablesys/compoventuals.git"
  },
  "dependencies": {
    "clean": "^4.0.2",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@types/ws": "^7.2.6",
    "tslib": "^2.0.1"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:ts": "tsc",
    "build:docs": "typedoc --out docs src/index.ts --tsconfig tsconfig.json",
    "tscw": "tsc --watch",
    "test": "npm-run-all test:*",
    "test:pkg": "pkg-ok",
    "test:format": "prettier --check .",
    "fix": "npm-run-all fix:*",
    "fix:format": "prettier --write .",
    "clean": "rimraf build docs"
  },
  "files": [
    "/build"
  ],
  "description": "CRDTs designed compositionally (server library)",
  "bugs": {
    "url": "https://github.com/composablesys/compoventuals/issues"
  },
  "homepage": "https://github.com/composablesys/compoventuals#readme",
  "keywords": [
    "crdts"
  ],
  "author": "Heather Miller",
  "license": "ISC"
}
