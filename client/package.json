{
  "name": "compoventuals-client",
  "version": "0.1.0",
  "main": "build/index.js",
  "unpkg": "build/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/composablesys/compoventuals.git"
  },
  "dependencies": {
    "bson": "^4.1.0",
    "clean": "^4.0.2",
    "protobufjs": "~6.10.1",
    "uuid": "^3.4.0",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@types/bson": "^4.0.2",
    "@types/uuid": "^3.4.0",
    "@types/ws": "^7.2.6",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.1",
    "tslib": "^2.0.1",
    "typescript": "^4.0.0"
  },
  "scripts": {
    "build": "npm run clean && npm run protobuild && (tsc || true) && cp src/proto_compiled.js build/src/proto_compiled.js",
    "clean": "(rm -r ./build || true)",
    "test": "node build/test/test.js",
    "bench": "node build/test/benchmark/benchmark.js",
    "tsc": "tsc",
    "tscw": "tsc --watch",
    "protobuild": "(node_modules/protobufjs/bin/pbjs -t static-module -w commonjs -o src/proto_compiled.js `find src -name '*.proto'`) && (node_modules/protobufjs/bin/pbts -o src/proto_compiled.d.ts src/proto_compiled.js)"
  },
  "files": [
    "build/src/**/*"
  ],
  "description": "CRDTs designed compositionally (client library)",
  "bugs": {
    "url": "https://github.com/composablesys/compoventuals/issues"
  },
  "homepage": "https://github.com/composablesys/compoventuals#readme",
  "directories": {
    "test": "test"
  },
  "keywords": [
    "crdts"
  ],
  "author": "Heather Miller",
  "license": "ISC"
}
