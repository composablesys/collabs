{
  "name": "compoventuals-client",
  "version": "0.1.0",
  "description": "CRDTs designed compositionally (client library)",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "unpkg": "build/src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/composablesys/compoventuals.git"
  },
  "dependencies": {
    "bson": "^4.1.0",
    "clean": "^4.0.2",
    "protobufjs": "~6.10.1",
    "uuid": "^3.4.0",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@types/bson": "^4.0.2",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@types/uuid": "^3.4.0",
    "@types/ws": "^7.2.6",
    "chai": "^4.2.0",
    "mkdirp": "^1.0.4",
    "mocha": "^8.2.1",
    "tslib": "^2.0.1"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:protobuf": "npm-run-all build:protobuf:prepare build:protobuf:js build:protobuf:ts",
    "build:protobuf:prepare": "mkdirp generated",
    "build:protobuf:js": "pbjs -t static-module -w commonjs -o generated/proto_compiled.js src/**/*.proto",
    "build:protobuf:ts": "pbts -o generated/proto_compiled.d.ts generated/proto_compiled.js",
    "build:ts": "tsc --project tsconfig.build.json",
    "clean": "rimraf generated build",
    "test": "npm-run-all test:*",
    "test:unit": "mocha",
    "test:pkg": "pkg-ok",
    "test:format": "prettier --check .",
    "fix": "npm-run-all fix:*",
    "fix:format": "prettier --write .",
    "tscw": "tsc --project tsconfig.build.json --watch"
  },
  "files": [
    "/build"
  ],
  "bugs": {
    "url": "https://github.com/composablesys/compoventuals/issues"
  },
  "homepage": "https://github.com/composablesys/compoventuals#readme",
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "keywords": [
    "crdts"
  ],
  "author": "Heather Miller",
  "license": "ISC"
}
