{
  "name": "compoventuals-webrtc-client",
  "version": "0.1.0",
  "description": "CRDTs designed compositionally (WebRTC network client)",
  "author": "Compoventuals contributors",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/composablesys/compoventuals/issues"
  },
  "homepage": "https://github.com/composablesys/compoventuals#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/composablesys/compoventuals.git"
  },
  "keywords": [
    "crdts"
  ],
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "unpkg": "build/src/index.js",
  "dependencies": {
    "compoventuals": "^0.1.0",
    "protobufjs": "~6.10.1",
    "ws": "^7.4.6",
    "@types/ws": "^7.2.6",
    "reconnecting-websocket": "^4.4.0"
  },
  "devDependencies": {
    "tslib": "^2.0.1"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:protobuf": "npm-run-all build:protobuf:prepare build:protobuf:js build:protobuf:ts",
    "build:protobuf:prepare": "mkdirp generated",
    "build:protobuf:js": "pbjs -t static-module -w commonjs -o generated/proto_compiled.js `find src -name '*.proto'`",
    "build:protobuf:ts": "pbts -o generated/proto_compiled.d.ts generated/proto_compiled.js",
    "build:ts": "tsc --project tsconfig.json",
    "build:docs": "typedoc --out docs src/index.ts --tsconfig tsconfig.json",
    "tscw": "tsc --watch",
    "test": "npm-run-all test:*",
    "test:pkg": "pkg-ok",
    "test:format": "prettier --check .",
    "fix": "npm-run-all fix:*",
    "fix:format": "prettier --write .",
    "clean": "rimraf generated build docs"
  },
  "files": [
    "/build"
  ],
  "directories": {
    "lib": "src"
  }
}
