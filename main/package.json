{
  "name": "compoventuals",
  "version": "0.1.0",
  "description": "CRDTs designed compositionally (main library)",
  "author": "Compoventuals contributors",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/composablesys/compoventuals/issues"
  },
  "homepage": "https://github.com/composablesys/compoventuals#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/composablesys/compoventuals.git"
  },
  "keywords": [
    "crdts"
  ],
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "unpkg": "build/src/index.js",
  "dependencies": {
    "buffer": "^6.0.3",
    "crypto-random-string": "~3.3.1",
    "protobufjs": "~6.10.1",
    "seedrandom": "^3.0.5",
    "@types/seedrandom": "^2.4.28"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "chai": "^4.2.0",
    "mkdirp": "^1.0.4",
    "mocha": "^8.2.1",
    "tslib": "^2.0.1"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:protobuf": "npm-run-all build:protobuf:prepare build:protobuf:js build:protobuf:ts",
    "build:protobuf:prepare": "mkdirp generated",
    "build:protobuf:js": "pbjs -t static-module -w commonjs -o generated/proto_compiled.js `find src -name '*.proto'`",
    "build:protobuf:ts": "pbts -o generated/proto_compiled.d.ts generated/proto_compiled.js",
    "build:ts": "tsc --project tsconfig.build.json",
    "build:docs": "typedoc --out docs src/index.ts --tsconfig tsconfig.build.json",
    "clean": "rimraf generated build docs",
    "test": "npm-run-all test:*",
    "test:unit": "mocha",
    "test:pkg": "pkg-ok",
    "test:format": "prettier --check .",
    "fix": "npm-run-all fix:*",
    "fix:format": "prettier --write .",
    "tscw": "tsc --project tsconfig.build.json --watch"
  },
  "files": [
    "/build"
  ],
  "directories": {
    "lib": "src",
    "test": "test"
  }
}
