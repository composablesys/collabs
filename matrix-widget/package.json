{
  "name": "compoventuals-matrix-widget",
  "version": "0.1.0",
  "description": "CRDTs designed compositionally (Matrix widget network)",
  "author": "Compoventuals contributors",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/composablesys/compoventuals/issues"
  },
  "homepage": "https://github.com/composablesys/compoventuals#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/composablesys/compoventuals.git"
  },
  "keywords": [
    "crdts"
  ],
  "module": "build/esm/src/index.js",
  "browser": "build/esm/src/index.js",
  "types": "build/esm/src/index.d.ts",
  "main": "build/commonjs/src/index.js",
  "files": [
    "/build"
  ],
  "directories": {
    "lib": "src"
  },
  "sideEffects": false,
  "dependencies": {
    "tslib": "^2.0.1",
    "compoventuals": "^0.1.0",
    "buffer": "^6.0.3",
    "matrix-widget-api": "^0.1.0-beta.16"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "typescript": "^4.3.5",
    "typedoc": "^0.21.4",
    "pkg-ok": "^2.3.1",
    "prettier": "^2.2.1",
    "rimraf": "^2.7.1"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:ts": "tsc -p tsconfig.json && tsc -p tsconfig.commonjs.json",
    "build:docs": "typedoc --out docs src/index.ts --tsconfig tsconfig.json",
    "test": "npm-run-all test:*",
    "test:pkg": "pkg-ok",
    "test:format": "prettier --check .",
    "fix": "npm-run-all fix:*",
    "fix:format": "prettier --write .",
    "clean": "rimraf build docs"
  }
}
