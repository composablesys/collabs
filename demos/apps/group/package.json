{
  "license": "Apache-2.0",
  "dependencies": {
    "@collabs/collabs": "0.12.0",
    "@collabs/storage": "0.12.0",
    "@collabs/ws-client": "0.12.0",
    "@collabs/ws-server": "0.12.0",
    "jquery": "^3.6.0",
    "protobufjs": "~6.9.0",
    "ts-node": "^10.1.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/jquery": "^3.5.5",
    "@types/node": "^15.6.1",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.16.2",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "npm-run-all": "^4.1.5",
    "mkdirp": "^1.0.4",
    "prettier": "^2.2.1",
    "rimraf": "^2.7.1",
    "source-map-loader": "^3.0.0",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.8.0"
  },
  "scripts": {
    "start": "ts-node --project tsconfig.commonjs.json bin/server.ts",
    "dev": "npm-run-all dev:*",
    "dev:protobuf": "npm run build:protobuf",
    "dev:webpack": "cross-env TS_NODE_PROJECT='tsconfig.commonjs.json' webpack",
    "build": "npm-run-all build:*",
    "build:protobuf": "npm-run-all build:protobuf:*",
    "build:protobuf:prepare": "mkdirp generated",
    "build:protobuf:js": "pbjs -t static-module -w es6 -o generated/proto_compiled.js `find src -name '*.proto'`",
    "build:protobuf:ts": "pbts -o generated/proto_compiled.d.ts generated/proto_compiled.js",
    "build:webpack": "cross-env TS_NODE_PROJECT='tsconfig.commonjs.json' webpack --mode production --devtool source-map",
    "test": "npm-run-all test:*",
    "test:format": "prettier --check .",
    "fix": "npm-run-all fix:*",
    "fix:format": "prettier --write .",
    "clean": "rimraf dist generated"
  }
}
