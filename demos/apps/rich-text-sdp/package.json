{
  "license": "Apache-2.0",
  "dependencies": {
    "@collabs/collabs": "0.9.0",
    "@collabs/crdts": "0.9.0",
    "@collabs/container": "0.9.0",
    "protobufjs": "~6.9.0",
    "quill": "^1.3.6"
  },
  "devDependencies": {
    "@types/quill": "^2.0.8",
    "@collabs/container-testing-server": "0.9.0",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.8.0",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.16.2",
    "cross-env": "^7.0.3",
    "ts-node": "^10.1.0",
    "@types/node": "^15.6.1",
    "typescript": "^4.3.5",
    "ts-loader": "^9.2.5",
    "source-map-loader": "^3.0.0",
    "css-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "react-dev-utils": "^12.0.0",
    "@types/react-dev-utils": "^9.0.8",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "@types/copy-webpack-plugin": "^8.0.1",
    "pkg-ok": "^2.3.1",
    "prettier": "^2.2.1",
    "rimraf": "^2.7.1"
  },
  "scripts": {
    "start": "container-testing-server dist/rich_text.html",
    "dev": "npm-run-all dev:*",
    "dev:protobuf": "npm run build:protobuf",
    "dev:webpack": "cross-env TS_NODE_PROJECT='tsconfig-for-webpack-config.json' webpack",
    "build": "npm-run-all build:*",
    "build:protobuf": "npm-run-all build:protobuf:*",
    "build:protobuf:prepare": "mkdirp generated",
    "build:protobuf:js": "pbjs -t static-module -w es6 -o generated/proto_compiled.js `find src -name '*.proto'`",
    "build:protobuf:ts": "pbts -o generated/proto_compiled.d.ts generated/proto_compiled.js",
    "build:webpack": "cross-env TS_NODE_PROJECT='tsconfig-for-webpack-config.json' webpack --mode production --devtool inline-nosources-source-map",
    "test": "npm-run-all test:*",
    "test:pkg": "pkg-ok",
    "test:format": "prettier --check .",
    "fix": "npm-run-all fix:*",
    "fix:format": "prettier --write .",
    "clean": "rimraf dist"
  }
}
