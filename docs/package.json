{
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typedoc": "^0.22.15"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:typedoc": "node ../foreach-workspace.js publish -s publishNoTypedoc \"npm run build:typedoc:one --\"",
    "build:typedoc:one": "bash -c 'typedoc --out generated/api/${1} ../${1}/src/index.ts --tsconfig ../${1}/tsconfig.json' @",
    "build:sphinx": "make html",
    "clean": "rimraf generated && make clean"
  }
}
