{
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typedoc": "^0.22.15"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:prepare": "make clean",
    "build:sphinx": "make html",
    "build:typedoc": "node ../foreach-workspace.js publish \"npm run build:typedoc:one --\"",
    "build:typedoc:one": "bash -c 'typedoc --options typedoc.json --out build/html/api/${1} ../${1}/src/index.ts --tsconfig ../${1}/tsconfig.json' @",
    "test": "",
    "fix": "",
    "clean": "rimraf build generated"
  }
}
