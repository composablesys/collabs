{
  "name": "compoventuals-benchmarks",
  "version": "0.1.0",
  "description": "CRDTs designed compositionally (benchmarks)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/composablesys/compoventuals.git"
  },
  "dependencies": {
    "compoventuals-client": "^0.1.0",
    "benchmark": "^2.1.4",
    "clean": "^4.0.2",
    "mathjs": "^9.2.0",
    "csv-write-stream": "^2.0.0",
    "uuid": "^3.4.0",
    "arg": "^5.0.0",
    "memory-streams": "^0.1.3",
    "seedrandom": "^3.0.5"
  },
  "devDependencies": {
    "@types/benchmark": "^2.1.0",
    "@types/glob": "^7.1.3",
    "@types/mathjs": "^6.0.11",
    "tslib": "^2.0.1",
    "@types/seedrandom": "^2.4.28"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:ts": "tsc",
    "build:docs": "typedoc --out docs src/framework.ts",
    "clean": "rimraf build docs",
    "test": "npm-run-all test:*",
    "test:format": "prettier --check .",
    "fix": "npm-run-all fix:*",
    "fix:format": "prettier --write .",
    "tscw": "tsc --project tsconfig.build.json --watch",
    "start": "node --expose-gc build/src/main.js",
    "dryrun": "npm start -- . test --no-write"
  },
  "bugs": {
    "url": "https://github.com/composablesys/compoventuals/issues"
  },
  "homepage": "https://github.com/composablesys/compoventuals#readme",
  "keywords": [
    "crdts"
  ],
