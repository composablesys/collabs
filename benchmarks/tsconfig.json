{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "build",
    "noEmit": false,
    "typeRoots": ["./src/@types"],
    "allowJs": true,
    "noImplicitAny": false,
    /*
     * Necessary for TensorFlow.js.
     * See https://www.tensorflow.org/js/tutorials/setup#typescript
     */
    "skipLibCheck": true
  },
  "include": ["src"]
}
